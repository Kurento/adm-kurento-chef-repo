{
  "name":"kurento-java-develop",
  "description":"Configuration for building kurento with java & javascript",
  "json_class":"Chef::Role",
  "default_attributes":{},
  "override_attributes": {
    "java":{
      "install_flavor":"openjdk",
      "jdk_version":"7"
    },
    "maven":{
      "setup_bin":"true"
    }
  },
  "chef_type":"role",
  "run_list": [ 
    "recipe[apt]",
    "recipe[java]",
    "recipe[curl]",
    "recipe[mongodb::10gen_repo]",
    "recipe[mongodb::default]",
    "recipe[python]",
    "recipe[jenkins-configurer::selenium]",
    "recipe[jenkins-configurer::documentation]"
  ],
  "env_run_lists":{}
}

